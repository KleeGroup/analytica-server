<?xml version="1.0" encoding="utf-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
		<el-resolver>kasperimpl.jsf.component.ComponentELResolver</el-resolver>
	</application>
	<converter>		
		<converter-id>thermometerConverter</converter-id>
		<converter-class>com.kleegroup.analyticaimpl.ui.chart.thermometer.ThermometerConverter</converter-class>
	</converter>
	<converter>		
		<converter-id>amcLineChartConverter</converter-id>
		<converter-class>com.kleegroup.analyticaimpl.ui.chart.amchart.LineChartConverter</converter-class>
	</converter>
	<converter>		
		<converter-id>flotrLineChartConverter</converter-id>
		<converter-class>com.kleegroup.analyticaimpl.ui.chart.flotr.LineChartConverter</converter-class>
	</converter>
	<converter>		
		<converter-id>flotrLineChartStackedConverter</converter-id>
		<converter-class>com.kleegroup.analyticaimpl.ui.chart.flotr.LineChartStackedConverter</converter-class>
	</converter>
	
	<converter>		
		<converter-id>flotrTransposedLineChartConverter</converter-id>
		<converter-class>com.kleegroup.analyticaimpl.ui.chart.flotr.TransposedLineChartConverter</converter-class>
	</converter>
	
	<converter>		
		<converter-id>jQuerySparkLineConverter</converter-id>
		<converter-class>com.kleegroup.analyticaimpl.ui.chart.sparkline.SparkLineConveter</converter-class>
	</converter>
	
</faces-config>