<div class="widget-inner  widget-size-1x1">
  <div class="ui-widget-header ">
    <i class="{{icon}}"></i><h>{{title}}</h>
  </div> 
  <!--<a class="led success" href="#"></a>-->
  <section class="widget-body">
    <div class="widget-canvas ass">
      <div class="positive text-center t-size-x72">
        <span class="text-center">
          {{mean.data}}
      </span>
      </div>
      <div>
        <table class = "table ">
          <thead>
            <tr>
              <th class = "text-left"><span class = "label">{{max.label}}</span></th>
              <th class = "text-left "><span class = "label">{{min.label}}</span></th>
            </tr>
          </thead>
          <tbody>
          <tr >
            <td class = "text-left">{{max.data}}</td>
            <td class = "text-left">{{min.data}}</td>
          </tr>
        </tbody>
        </table>
      </div>
    </div><!-- .widget-canvas -->
  </section>    
</div>

